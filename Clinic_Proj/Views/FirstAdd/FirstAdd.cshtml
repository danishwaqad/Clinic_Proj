            //=============Mode Data Get By ID==========
            $scope.GetModeTxtByID = function (ID) {
                var displayReq = {
                    method: "POST",
                    url: "/FirstAdd/Get_Modebyid",
                    data: {
                        TokenNo: ID
                    }
                }
                $http(displayReq).then(function (Return) {
                    var dt = angular.fromJson(Return.data);
                    if (dt.length > 0) {
                        $scope.txtMode = dt[0].CheckMode;
                        if ($scope.txtMode == 'IN') {
                            //$scope.txtBeforeIN = false;
                            $scope.Notification('warning', 'System Alert', 'Before You Add Service Check Mode And Recomendation');
                            $scope.ForInBtn = false;
                            $scope.ForOutBtn = true;
                        }
                        else if ($scope.txtMode == 'OUT') {
                            //$scope.txtAfterOut = false; 
                            $scope.Notification('warning', 'System Alert', 'First You Out This Command Then Other Recoemndation Add As a Service');
                            $scope.ForInBtn = true;
                            $scope.ForOutBtn = false;
                        }
                        $scope.CUSaveBtn = false;
                    }
                    else {
                        $scope.CUSaveBtn = false;
                        $scope.txtMode = '';
                    }

                }, function myError(Return) {
                    $scope.Notification('error', 'Error Code LC0001', Return.data);
                });
            };
