    <!--row-->
    <div class="row">
        <div class="col-lg-12">
            <div class="card">
                <div class="card-header bg-info">
                    <h4 class="m-b-0 text-white">CNIC Registeration</h4>
                </div>
                <div class="card-body">
                    <div id="PatientName" class="modal fade" tabindex="-1" role="dialog" aria-labelledby="myLargeModalLabel" aria-hidden="true" style="display: none;">
                        <div class="modal-dialog modal-xl">
                            <div class="modal-content">
                                <div class="modal-header">
                                    <h4 class="modal-title" id="myLargeModalLabel">Select Patient Name</h4>
                                    <button type="button" class="close" data-dismiss="modal" aria-hidden="true">×</button>
                                </div>
                                <div class="modal-body">
                                    <div class="row">
                                        <div class="col-md-7">

                                        </div>
                                        <div class="col-md-1">
                                            <label for="CompSearch">Search:</label>
                                        </div>
                                        <div class="col-md-4 form-material">
                                            <input type="text" class="form-control" id="UserSearch" ng-model="TxtTokenSearch" />
                                        </div>
                                    </div>
                                    <div class="table-responsive-sm">
                                        <table class="table-sm table-hover table-bordered text-dark">
                                            <thead>
                                                <tr>
                                                    <th>Reg #</th>
                                                    <th>Patient Name</th>
                                                </tr>
                                            </thead>
                                            <tbody>
                                                <tr ng-repeat="item in dtGetAll | filter:TxtTokenSearch">
                                                    <td><button ng-click="GetDataByID(item.RegNo)" class="btn btn-link" data-dismiss="modal">{{item.RegNo}}</button></td>
                                                    <td>{{item.FullName}}</td>
                                                </tr>
                                            </tbody>
                                        </table>
                                    </div>
                                </div>
                                <div class="modal-footer">
                                    <button type="button" class="btn btn-danger waves-effect text-left" data-dismiss="modal">Close</button>
                                </div>
                            </div>
                            <!-- /.modal-content -->
                        </div>
                        <!-- /.modal-dialog -->
                    </div>
                    <div id="PatientCnicModal" class="modal fade" tabindex="-1" role="dialog" aria-labelledby="myLargeModalLabel" aria-hidden="true" style="display: none;">
                        <div class="modal-dialog modal-xl">
                            <div class="modal-content">
                                <div class="modal-header">
                                    <h4 class="modal-title" id="myLargeModalLabel">Select Patient CNIC Already Register</h4>
                                    <button type="button" class="close" data-dismiss="modal" aria-hidden="true">×</button>
                                </div>
                                <div class="modal-body">
                                    <div class="row">
                                        <div class="col-md-7">

                                        </div>
                                        <div class="col-md-1">
                                            <label for="CompSearch">Search:</label>
                                        </div>
                                        <div class="col-md-4 form-material">
                                            <input type="text" class="form-control" id="UserSearch" ng-model="TxtinfoSearch" />
                                        </div>
                                    </div>
                                    <div class="table-responsive-sm">
                                        <table class="table-sm table-hover table-bordered text-dark">
                                            <thead>
                                                <tr>
                                                    <th>CNIC</th>
                                                    <th>Reg No</th>
                                                    <th>FullName</th>
                                                    <th>Gender</th>
                                                    <th>Age</th>
                                                    <th>Address</th>
                                                    <th>SiteID</th>
                                                </tr>
                                            </thead>
                                            <tbody>
                                                <tr ng-repeat="item in dtCNGetAll | filter:TxtinfoSearch">
                                                    <td><button ng-click="GetCNDataByID(item.CNIC)" class="btn btn-link" data-dismiss="modal">{{item.CNIC}}</button></td>
                                                    <td>{{item.RegNo}}</td>
                                                    <td>{{item.FullName}}</td>
                                                    <td>{{item.Gender}}</td>
                                                    <td>{{item.Age}}</td>
                                                    <td>{{item.Address}}</td>
                                                    <td>{{item.SiteID}}</td>
                                                </tr>
                                            </tbody>
                                        </table>
                                    </div>
                                </div>
                                <div class="modal-footer">
                                    <button type="button" class="btn btn-danger waves-effect text-left" data-dismiss="modal">Close</button>
                                </div>
                            </div>
                            <!-- /.modal-content -->
                        </div>
                        <!-- /.modal-dialog -->
                    </div>
                    <div id="PatientPanal" class="modal fade show" ng-if="ShowPatient == true" role="dialog" aria-labelledby="myLargeModalLabel" style="display: block; padding-right: 17px;" aria-modal="true">
                        <div class="modal-dialog-scrollable">
                            <div class="modal-dialog modal-xl modal-x">
                                <div class="modal-content">
                                    <div class="modal-header">
                                        <h4 class="modal-title" id="myLargeModalLabel">Select Panal Patients</h4>
                                        <button type="button" ng-click="ClosePanalPatientModal()" class="btn btn-danger" aria-hidden="true">×</button>
                                    </div>
                                    <div class="modal-body">
                                        <div class="row">
                                            <div class="col-md-7">

                                            </div>
                                            <div class="col-md-1">
                                                <label for="CompSearch">Search:</label>
                                            </div>
                                            <div class="col-md-4 form-material">
                                                <input type="text" class="form-control" id="PatTypeSearch" ng-model="TxtAvailSearch" />
                                            </div>
                                        </div>
                                        <div class="table-responsive-sm">
                                            <table class="table-sm table-hover table-bordered text-dark">
                                                <thead>
                                                    <tr>
                                                        <th>CNIC</th>
                                                        <th>Dependent CNIC</th>
                                                        <th>Name</th>
                                                        <th>Gender</th>
                                                        <th>Tel1</th>
                                                        <th>Tel2</th>
                                                        <th>Address</th>
                                                        <th>City</th>
                                                        <th>Country</th>
                                                        <th>DOB</th>
                                                        <th>Relation</th>
                                                        <th>Company</th>
                                                        <th>Remarks</th>
                                                    </tr>
                                                </thead>
                                                <tbody>
                                                    <tr ng-repeat="item in dtPanlGetPatAvail | filter:TxtAvailSearch">
                                                        <td><button ng-click="REG_CNIC_PanalData(item.CNIC)" class="btn btn-link" data-dismiss="modal">{{item.CNIC}}</button></td>
                                                        <td>{{item.EmpCNIC}}</td>
                                                        <td>{{item.Name}}</td>
                                                        <td>{{item.Gender}}</td>
                                                        <td>{{item.Tel1}}</td>
                                                        <td>{{item.Tel2}}</td>
                                                        <td>{{item.Address}}</td>
                                                        <td>{{item.City}}</td>
                                                        <td>{{item.Country}}</td>
                                                        <td>{{item.DOB | date:dd-MMM-yyyy}} [ {{item.Age}} ]</td>
                                                        <td>{{item.Relation}}</td>
                                                        <td>{{item.CompanyName}}</td>
                                                        <td>{{item.Remarks}}</td>
                                                    </tr>
                                                </tbody>
                                            </table>
                                        </div>
                                    </div>
                                    <div class="modal-footer">
                                        <button ng-click="ClosePanalPatientModal()" class="btn btn-danger">Close</button>
                                    </div>
                                </div>
                                <!-- /.modal-content -->
                            </div>
                        </div>
                    </div>
                    @*======================================*@
                    <!--Insert Whats App-->
                    <div id="WhatsApp" class="modal fade" ng-if="ShowWhatsCnt == true" tabindex="-1" role="dialog" aria-labelledby="myLargeModalLabel" aria-hidden="true" style="display: none;">
                        <div class="modal-dialog modal-md">
                            <div class="modal-content">
                                <div class="modal-header">
                                    <h4 class="modal-title" id="myLargeModalLabel">WhatsApp Registration</h4>
                                </div>
                                <div class="form-group row">
                                    <div class="modal-body">
                                        <div class="row p-t-20">
                                            <div class="col-md-1"></div>
                                            <div class="col-md-1">
                                                <label class="control-label text-dark" for="TypeID">Contact Types:</label>
                                            </div>
                                            <div class="col-md-1"></div>
                                            <div class="col-md-8">
                                                <select class="form-control" ng-model="txtContactType">
                                                    <option value="0" label="-Please Select-"></option>
                                                    <option ng-repeat="item in dtGetContactType" value="{{item.TypeID}}">{{item.TypeID}}</option>
                                                </select>
                                            </div>
                                        </div>
                                        <div class="row p-t-20">
                                            <div class="col-md-1"></div>
                                            <div class="col-sm-1">
                                                <label class="control-label text-dark" for="Telephone">Tel #:</label>
                                            </div>
                                            <div class="col-md-1"></div>
                                            <div class="col-md-8">
                                                <div class="input-group">
                                                    <input type="text" maxlength="12" name="TelephoneWht" class="form-control font-weight-bold" placeholder="3411234567" ng-model="txtTelephoneW" />
                                                    @* ng-blur="getTelephoneLength(txtTelephoneW)" *@
                                                </div>
                                            </div>
                                        </div>
                                        <div class="row p-t-20">
                                            <div class="col-md-1"></div>
                                            <div class="col-sm-1">
                                                <label class="control-label text-dark" for="Email">Email:</label>
                                            </div>
                                            <div class="col-md-1"></div>
                                            <div class="col-md-8">
                                                <div class="input-group">
                                                    <input type="email" id="Email" class="form-control" ng-model="txtEmailW" />
                                                </div>
                                            </div>
                                        </div>
                                        <div class="row p-t-20">
                                            <div class="col-md-1"></div>
                                            <div class="col-sm-1">
                                                <label class="control-label text-dark" for="Remarks">Remarks:</label>
                                            </div>
                                            <div class="col-md-1"></div>
                                            <div class="col-md-8">
                                                <div class="input-group">
                                                    <input type="text" id="Remarks" class="form-control" ng-model="txtCRemarks" />
                                                </div>
                                            </div>
                                        </div>
                                        <div class="row p-t-20">
                                            <div class="col-md-5"></div>
                                            <div class="col-md-3">
                                                <div class="form-group">
                                                    <button ng-click="On_ClearC()" class="btn btn-outline-warning form-control"><i class="icon-refresh"></i> Clear </button>
                                                </div>
                                            </div>
                                            <div class="col-md-3">
                                                <div class="form-group">
                                                    <button type="button" ng-click="savedataContact(txtContactType,txtTelephoneW,txtEmailW,txtCRemarks)" class="btn btn-outline-success form-control" data-dismiss="modal"><i class="fa fa-check"></i>Save</button>
                                                </div>
                                            </div>
                                        </div>
                                    </div>
                                </div>
                            </div>
                            <!-- /.modal-content -->
                        </div>
                    </div>
                    @*======================================*@
                    <!--Patient CNIC Are Availabe-->
                    <div id="PatientAvai" class="modal fade show" ng-if="ShowCNICReg == true" role="dialog" aria-labelledby="myLargeModalLabel" style="display: block; padding-right: 17px;" aria-modal="true">
                        <div class="modal-dialog modal-dialog-centered modal-sm">
                            <div class="modal-content">
                                <div class="modal-header">
                                    <h4 class="modal-title" id="myLargeModalLabel">CNIC Registeration</h4>
                                </div>
                                <div class="modal-body">
                                    <div class="row">
                                        <div class="col-md-12">
                                            <p>CNIC is Already Registered...</p>
                                            <p>Do You Want To Proceed For Patient Registration..?</p>
                                            <p>{{ WhatsAppLabel }}</p>
                                        </div>
                                    </div>
                                </div>
                                <div class="modal-footer">
                                    <button type="button" class="btn btn-light" style="color:#25D366" data-toggle="modal" ng-hide="WhatsAppBtn" ng-click="CheckWhtsProceedModal(txtRegNo,txtRegDate)" data-target="#WhatsApp" href="javascript:void(0)"><i class="fab fa-whatsapp-square"></i> WhatsApp</button>
                                    <button ng-click="REG_CNIC(txtCnic)" class="btn btn-warning">Proceed</button>
                                    <button type="button" ng-click="CloseCNICRegModal()" class="btn btn-danger waves-effect text-left" data-dismiss="modal">Close</button>
                                </div>
                            </div>
                            <!-- /.modal-content -->
                        </div>
                    </div>
                    @*======================================*@
                    <!--Patient Proceed Or Not-->
                    @*<div id="PatientProcNot" class="modal fade show" role="dialog" aria-labelledby="myLargeModalLabel" style="display: block; padding-right: 17px;" aria-modal="true">*@
                    <div id="PatientProcNot" class="modal fade" tabindex="-1" role="dialog" aria-labelledby="myLargeModalLabel" aria-hidden="true" style="display: none;">
                        <div class="modal-dialog modal-dialog-centered modal-md">
                            <div class="modal-content">
                                <div class="modal-header">
                                    <h4 class="modal-title" id="myLargeModalLabel">You Want To Continue Your Registration</h4>
                                    <button type="button" class="close" data-dismiss="modal" aria-hidden="true">×</button>
                                </div>
                                <div class="modal-body">
                                    <div class="row">
                                        <div class="col-md-12">
                                            <p>For Self Registration Please Click (Self)Token GeneRate</p>
                                            <p>For Other Registration Please Click (Other)Token GeneRate</p>
                                        </div>
                                    </div>
                                </div>
                                <div class="modal-footer">
                                    <button ng-click="GetPatType();NewToken(txtCnic,'Self');GetDoctorname();InfoTransfer(txtCnic);GetBank();GetPayMeth()" alt="default" data-toggle="modal" href="javascript:void(0)" data-target="#PatientModlDes" class="btn btn-outline-success"><i class="fa fa-check"></i>(Self) Token GeneRate</button>
                                    <button ng-click="TrasferCNData()" class="btn btn-success">(Other)Token GeneRate</button>
                                    @*<button ng-click="CloseCNICRegModal()" class="btn btn-danger">Close</button>*@
                                    @*<div class="modal-footer">*@
                                    <button type="button" class="btn btn-danger" data-dismiss="modal">Close</button>

                                </div>
                            </div>
                            <!-- /.modal-content -->
                        </div>
                    </div>
                    @* ===============================CNIC For Further Proceeding======================= *@
                    <div id="PatientModlDes" class="modal fade" tabindex="-1" role="dialog" aria-labelledby="myLargeModalLabel" aria-hidden="true" style="display: none;">
                        <div class="modal-dialog modal-xl modal-x">
                            <div class="modal-content">
                                <div class="modal-header">
                                    <h4 class="modal-title" id="myLargeModalLabel">Register Self Token</h4>
                                    <button type="button" ng-click="On_ClearForPopup()" class="close" data-dismiss="modal" aria-hidden="true">×</button>
                                </div>
                                <form name="PatientSelfRegister">
                                    <div class="form-body form-group">
                                        <div class="modal-body">
                                            <div id="Banks" class="modal fade" tabindex="-1" role="dialog" aria-labelledby="myLargeModalLabel" aria-hidden="true" style="display: none;">
                                                <div class="modal-dialog modal-xl">
                                                    <div class="modal-content">
                                                        <div class="modal-header">
                                                            <h4 class="modal-title" id="myLargeModalLabel">Select Bank Name</h4>
                                                            <button type="button" class="close" data-dismiss="modal" aria-hidden="true">×</button>
                                                        </div>
                                                        <div class="modal-body">
                                                            <div class="row">
                                                                <div class="col-md-7">

                                                                </div>
                                                                <div class="col-md-1">
                                                                    <label for="CompSearch">Search:</label>
                                                                </div>
                                                                <div class="col-md-4 form-material">
                                                                    <input type="text" class="form-control" id="UserSearch" ng-model="TxtBnkSearch" />
                                                                </div>
                                                            </div>
                                                            <div class="table-responsive-sm">
                                                                <table class="table-sm table-hover table-bordered text-dark">
                                                                    <thead>
                                                                        <tr>
                                                                            <th>Banks Name</th>
                                                                        </tr>
                                                                    </thead>
                                                                    <tbody>
                                                                        <tr ng-repeat="item in dtBankName | filter: TxtBnkSearch">
                                                                            <td><button ng-click="GetBankName(item.BankName)" class="btn btn-link" data-dismiss="modal">{{item.BankName}}</button></td>
                                                                        </tr>
                                                                    </tbody>
                                                                </table>
                                                            </div>
                                                        </div>
                                                        <div class="modal-footer">
                                                            <button type="button" class="btn btn-danger waves-effect text-left" data-dismiss="modal">Close</button>
                                                        </div>
                                                    </div>
                                                    <!-- /.modal-content -->
                                                </div>
                                                <!-- /.modal-dialog -->
                                            </div>
                                            <div class="row p-t-20">
                                                <div class="col-md-1"></div>
                                                <div class="col-md-1">
                                                    <label class="control-label text-dark" for="TokenNo">M.R #:</label>
                                                </div>
                                                <div class="col-md-3">
                                                    <div class="input-group">
                                                        <input type="text" id="TokenNo" class="form-control form-input-grey" ng-model="txtTokenNo" readonly />
                                                    </div>
                                                </div>
                                                <div class="col-md-1"></div>
                                                <div class="col-md-1">
                                                    <label class="control-label text-dark" for="PatType">Types:</label>
                                                </div>
                                                <div class="col-md-3">
                                                    <select class="form-control" ng-model="txtPatType" ng-click="TypeWiseSelection()" required>
                                                        @*<option value="0" label="-Please Select-"></option>*@
                                                        <option ng-repeat="item in dtGetTypeAll" value="{{item.PatientType}}">{{item.PatientType}}</option>
                                                    </select>
                                                </div>
                                            </div>
                                            <div class="row p-t-20">
                                                <div class="col-md-1"></div>
                                                <div class="col-sm-1">
                                                    <label class="control-label text-dark" for="FathName">Father Name:</label>
                                                </div>
                                                <div class="col-md-3">
                                                    <div class="input-group">
                                                        <input type="text" id="FathName" class="form-control form-input-grey" ng-model="txtFathname" />
                                                    </div>
                                                </div>
                                                <div class="col-md-1"></div>
                                                <div class="col-sm-1">
                                                    <label class="control-label text-dark" for="City">City:</label>
                                                </div>
                                                <div class="col-md-3">
                                                    <div class="input-group">
                                                        <input type="text" id="City" class="form-control form-input-grey" ng-model="txtCity" />
                                                    </div>
                                                </div>
                                            </div>
                                            <div class="row p-t-20">
                                                <div class="col-md-1"></div>
                                                <div class="col-sm-1">
                                                    <label class="control-label text-dark" for="Country">Country:</label>
                                                </div>
                                                <div class="col-md-3">
                                                    <div class="input-group">
                                                        <input type="text" id="Country" class="form-control form-input-grey" ng-model="txtCountry" />
                                                    </div>
                                                </div>

                                                <div class="col-md-1"></div>
                                                <div class="col-sm-1">
                                                    <label class="control-label text-dark" for="EmailID">Email:</label>
                                                </div>
                                                <div class="col-md-3">
                                                    <div class="input-group">
                                                        <input type="text" id="EmailID" class="form-control form-input-grey" ng-model="txtEmailID" />
                                                    </div>
                                                </div>
                                            </div>
                                            <div class="row p-t-20">
                                                <div class="col-md-1"></div>
                                                <div class="col-md-1">
                                                    <label class="control-label text-dark" for="Tel1">Cell Ph# 1:</label>
                                                </div>
                                                <div class="col-md-3">
                                                    <div class="input-group">
                                                        <input type="text" maxlength="12" id="Tel1" placeholder="3001234567" class="form-control form-input-grey" ng-blur="getTel1Length()" ng-model="txtTel1" />
                                                    </div>
                                                </div>
                                                <div class="col-md-1"></div>
                                                <div class="col-md-1">
                                                    <label class="control-label text-dark" for="Tel2">Cell Ph# 2:</label>
                                                </div>
                                                <div class="col-md-3">
                                                    <div class="input-group">
                                                        <input type="text" maxlength="12" id="Tel2" placeholder="3001234567" class="form-control form-input-grey" ng-blur="getTel2Length()" ng-model="txtTel2" />
                                                    </div>
                                                </div>
                                            </div>
                                            <div class="row p-t-20">
                                                <div class="col-md-1"></div>
                                                <div class="col-sm-1">
                                                    <label class="control-label text-dark" for="Address">Address:</label>
                                                </div>
                                                <div class="col-md-8">
                                                    <div class="input-group">
                                                        <input type="text" id="Address" class="form-control form-input-grey" ng-model="txtAddress" />
                                                    </div>
                                                </div>
                                            </div>
                                            <div class="row p-t-20">
                                                <div class="col-md-1"></div>
                                                <div class="col-sm-1">
                                                    <label class="control-label text-dark" for="Remarks">Remarks<br />If Any:</label>
                                                </div>
                                                <div class="col-md-8">
                                                    <div class="input-group">
                                                        <input type="text" id="Remarks" class="form-control form-input-grey" ng-model="txtRemarks" />
                                                    </div>
                                                </div>
                                            </div>
                                            <div class="row p-t-20">
                                                <div class="col-md-1"></div>
                                                <div class="col-md-1">
                                                    <label class="control-label text-dark" for="DoctorName">Doctor Name:</label>
                                                </div>
                                                <div class="col-md-3">
                                                    <div class="input-group">
                                                        <input disabled type="text" id="DoctorID" class="form-control col-md-2 form-input-grey text-center" ng-model="txtDoctID" ng-disabled="DocIDDisable" readonly />
                                                        <select class="form-control" ng-model="txtDoctName" ng-disabled="DocNamSelDisable" ng-click="GetDoctorByid()">
                                                            <option value="0" label="-Please Select-"></option>
                                                            <option ng-repeat="item in dtGetDocName" value="{{item.DrID}}">{{item.DrName}}</option>
                                                        </select>
                                                    </div>
                                                </div>
                                                <div class="col-md-1"></div>
                                                <div class="col-md-1">
                                                    <label class="control-label text-dark" for="Charges">Fee Charges:</label>
                                                </div>
                                                <div class="col-md-3">
                                                    <div class="input-group">
                                                        <input disabled type="text" id="Charges" ng-disabled="DocFeeDisable" class="form-control form-input-grey text-center font-weight-bold" ng-model="txtCharges" readonly />
                                                    </div>
                                                </div>
                                            </div>
                                            <div class="row p-t-20">
                                                <div class="col-md-1"></div>
                                                <div class="col-md-1">
                                                    <label class="control-label text-dark" for="PaidFee">Paid Amount:</label>
                                                </div>
                                                <div class="col-md-3">
                                                    <div class="input-group">
                                                        <input type="number" id="PaidFee" class="form-control form-input-grey text-center font-weight-bold" ng-disabled="DocPaidDisable" ng-model="txtPaidFee" value="0" ng-blur="CalculateFee()" />
                                                    </div>
                                                </div>
                                                <div class="col-md-1"></div>
                                                <div class="col-md-1">
                                                    <label class="control-label text-dark" for="Discount">Discount:</label>
                                                </div>
                                                <div class="col-md-3">
                                                    <div class="input-group">
                                                        <select class="form-control form-input-grey" ng-disabled="DocDiscDisable" ng-model="DDLDisType">
                                                            <option value="0">-Select-</option>
                                                            <option value="%">%</option>
