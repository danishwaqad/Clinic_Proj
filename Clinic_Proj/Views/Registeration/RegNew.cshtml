
@{
    ViewBag.Title = "RegNew";
}
<div ng-app="RegistrationForm" ng-controller="Home">

    @*<div class="row page-titles">
        <div class="col-md-5 align-self-center">
            <h4 class="text-themecolor">CNIC Registeration</h4>
