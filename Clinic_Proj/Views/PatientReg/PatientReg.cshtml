                    <!--Patient Already Availabe-->
                    <div id="PatientAvai" class="modal fade show" ng-if="ShowPatient == true" role="dialog" aria-labelledby="myLargeModalLabel" style="display: block; padding-right: 17px;" aria-modal="true">
                        <div class="modal-dialog-scrollable">
                            <div class="modal-dialog modal-xl">
                                <div class="modal-content">
                                    <div class="modal-header">
                                        <h4 class="modal-title" id="myLargeModalLabel">Select Patient</h4>
                                        <button type="button" ng-click="ClosePatientModal()" class="btn btn-danger" aria-hidden="true">×</button>
                                    </div>
                                    <div class="modal-body">
                                        <div class="row">
                                            <div class="col-md-7">

                                            </div>
                                            <div class="col-md-1">
                                                <label for="CompSearch">Search:</label>
                                            </div>
                                            <div class="col-md-4 form-material">
                                                <input type="text" class="form-control" id="PatTypeSearch" ng-model="TxtAvailSearch" />
                                            </div>
                                        </div>
                                        <div class="table-responsive-sm">
                                            <table class="table-sm table-hover table-bordered text-dark">
                                                <thead>
                                                    <tr>
                                                        <th>Patient Name</th>
                                                        <th>Last Visit</th>
                                                        <th>CNIC</th>
                                                        <th>Patient Type</th>
                                                        <th>Doctor</th>
                                                        <th>Age</th>
                                                        <th>Relation</th>
                                                        <th>Site</th>
                                                    </tr>
                                                </thead>
                                                <tbody>
                                                    <tr ng-repeat="item in dtGetPatAvail | filter:TxtAvailSearch">
                                                        <td><button ng-click="GeExistdataid(item.CNIC,item.Relation,item.CCPL_ROW_ID)" class="btn btn-link" data-dismiss="modal">{{item.FullName}}</button></td>
                                                        <td>{{item.TokenDate | date:dd-MMM-yyyy}}</td>
                                                        <td>{{item.CNIC}}</td>
                                                        <td>{{item.PatType}}</td>
                                                        <td>{{item.DrName}}</td>
                                                        <td>{{item.DOB | date:dd-MMM-yyyy}} [ {{item.Age}} ]</td>
                                                        <td>{{item.Relation}}</td>
                                                        <td>{{item.SiteID}}</td>
                                                    </tr>
                                                </tbody>
                                            </table>
                                        </div>
                                    </div>
                                    <div class="modal-footer">
                                        <button ng-click="ClosePatientModal()" class="btn btn-danger">Close</button>
                                    </div>
                                </div>
                                <!-- /.modal-content -->
                            </div>
                        </div>
                    </div>

