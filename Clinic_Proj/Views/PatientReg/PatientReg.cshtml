                                                    <td><button ng-click="GetPatAvail_ByRelation(txtCNIC,item.RelationName)" class="btn btn-link" data-dismiss="modal">{{item.RelationName}}</button></td>
                                                    <td>{{item.InActive}}</td>
                                                </tr>
                                            </tbody>
                                        </table>
                                    </div>
