                                                    <td><button ng-click="GetPatByType(item.PatientType)" class="btn btn-link" data-dismiss="modal">{{item.PatientType}}</button></td>
                                                    <td>{{item.InActive}}</td>
                                                </tr>
                                            </tbody>
                                        </table>
                                    </div>
