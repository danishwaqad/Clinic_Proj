                                <button type="button" class="btn btn-danger waves-effect text-left" data-dismiss="modal">Close</button>
                            </div>
                        </div>
                        <!-- /.modal-content -->
                    </div>
                    <!-- /.modal-dialog -->
                </div>
                <div id="DiagnoseHead" class="modal fade" tabindex="-1" role="dialog" aria-labelledby="myLargeModalLabel" aria-hidden="true" style="display: none;">
                    <div class="modal-dialog modal-xl">
                        <div class="modal-content">
                            <div class="modal-header">
                                <h4 class="modal-title" id="myLargeModalLabel">Select Diagnose Header</h4>
                                <button type="button" class="close" data-dismiss="modal" aria-hidden="true">×</button>
                            </div>
                            <div class="modal-body">
                                <div class="row">
                                    <div class="col-md-7">

                                    </div>
                                    <div class="col-md-1">
                                        <label for="CompSearch">Search:</label>
                                    </div>
                                    <div class="col-md-4 form-material">
                                        <input type="text" class="form-control" id="UserSearch" ng-model="TxtHeadDiagSearch" />
                                    </div>
                                </div>
                                <div class="table-responsive-sm">
                                    <table class="table-sm table-hover table-bordered text-dark">
                                        <thead>
                                            <tr>
                                                <th>Header Name</th>
                                            </tr>
                                        </thead>
                                        <tbody>
                                            <tr ng-repeat="item in dtHeadGetAll | filter:TxtHeadDiagSearch">
                                                <td><button ng-click="GetHeadDataByID(item.HeaderID)" class="btn btn-link" data-dismiss="modal">{{item.HeaderName}}</button></td>
                                            </tr>
                                        </tbody>
                                    </table>
                                </div>
                            </div>
                            <div class="modal-footer">
                                <button type="button" class="btn btn-danger waves-effect text-left" data-dismiss="modal">Close</button>
                            </div>
                        </div>
                        <!-- /.modal-content -->
                    </div>
                    <!-- /.modal-dialog -->
                </div>
                <div id="DiagHead" class="modal fade" tabindex="-1" role="dialog" aria-labelledby="myLargeModalLabel" aria-hidden="true" style="display: none;">
                    <div class="modal-dialog modal-xl">
                        <div class="modal-content">
                            <div class="modal-header">
                                <h4 class="modal-title" id="myLargeModalLabel">Select Diagnose Header</h4>
                                <button type="button" class="close" data-dismiss="modal" aria-hidden="true">×</button>
                            </div>
                            <div class="modal-body">
                                <div class="row">
                                    <div class="col-md-7">

                                    </div>
                                    <div class="col-md-1">
                                        <label for="CompSearch">Search:</label>
                                    </div>
                                    <div class="col-md-4 form-material">
                                        <input type="text" class="form-control" id="UserSearch" ng-model="TxtHeadDiag1Search" />
                                    </div>
                                </div>
                                <div class="table-responsive-sm">
                                    <table class="table-sm table-hover table-bordered text-dark">
                                        <thead>
                                            <tr>
                                                <th>Header Name</th>
                                            </tr>
                                        </thead>
                                        <tbody>
                                            <tr ng-repeat="item in dtHeadDiagGetAll | filter:TxtHeadDiag1Search">
                                                <td><button ng-click="GetHeadDiagDataByID(item.HeaderID)" class="btn btn-link" data-dismiss="modal">{{item.HeaderName}}</button></td>
                                            </tr>
                                        </tbody>
                                    </table>
                                </div>
                            </div>
                            <div class="modal-footer">
                                <button type="button" class="btn btn-danger waves-effect text-left" data-dismiss="modal">Close</button>
                            </div>
                        </div>
                        <!-- /.modal-content -->
                    </div>
                    <!-- /.modal-dialog -->
                </div>
                <div class="card-header bg-info">
                    <h4 class="m-b-0 text-white">Create Diagnose Category</h4>
                </div>
                <div class="card-body">
                    <div class="form-body form-group">
                        <div class="row p-t-20">
                            <div class="col-md-6">
                                <form name="DiagnoseHead">
                                    <div class="row p-t-20">
                                        <div class="col-sm-2">
                                            <label class="control-label text-dark" for="HeaderID">Header ID:</label>
                                        </div>
                                        <div class="col-md-9">
                                            <div class="input-group">
                                                <input type="text" id="HeaderID" class="form-control form-input-grey text-center font-weight-bold" ng-model="txtHeaderID" required readonly />
                                                <div class="input-group-append">
                                                    <button type="button" class="btn btn-info" alt="default" data-toggle="modal" data-target="#DiagnoseHead" href="javascript:void(0)" ng-click="GetHeadAllData()"><i class="ti-search"></i></button>
                                                </div>
                                            </div>
                                        </div>
                                    </div>
                                    <div class="row p-t-20">
                                        <div class="col-sm-2">
                                            <label class="control-label text-dark" for="HeaderName">Header Name:</label>
                                        </div>
                                        <div class="col-md-9">
                                            <div class="input-group">
                                                <input type="text" id="HeaderName" class="form-control form-input-grey" ng-model="txtHeaderName" required />
                                            </div>
                                        </div>
                                    </div>
                                    <div class="row p-t-20">
                                        <div class="col-sm-2">
                                            <label class="control-label text-dark" for="InActive">InActive:</label>
                                        </div>
                                        <div class="col-md-9">
                                            <select id="Status" ng-model="txtInActive" class="btn btn-outline-secondary dropdown-toggle form-control form-input-grey">
                                                <option value="Active">Active</option>
                                                <option value="InActive">InActive</option>
                                            </select>
                                        </div>
                                    </div>
                                    <div class="row p-t-20">
                                        <div class="col-md-2"></div>
                                        <div class="col-md-3">
                                            <div class="form-group">
                                                <button ng-click="saveHeaddata()" ng-disabled="DiagnoseHead.$invalid" class="btn btn-outline-success form-control"><i class="fa fa-check"></i>Save</button>
                                            </div>
                                        </div>
                                        <div class="col-md-3">
                                            <div class="form-group">
                                                <button ng-click="On_ClearHead()" class="btn btn-outline-warning form-control"><i class="icon-refresh"></i> Clear </button>
                                            </div>
                                        </div>
                                        <div class="col-md-3">
                                            <div class="form-group">
                                                <button ng-disabled="DiagnoseHead.$invalid" ng-click="OnHeadDelete()" class="btn btn-outline-danger form-control"><i class="icon-trash"></i>Delete</button>
                                            </div>
                                        </div>
                                        <div class="col-md-3"></div>
                                    </div>
                                </form>
                            </div>
                            <div class="col-md-6">
                                <form name="DiagnoseDisease">
                                    <div class="row p-t-20">
                                        <div class="col-sm-2">
                                            <label class="control-label text-dark" for="HeaderID">Header ID:</label>
                                        </div>
                                        <div class="col-md-9">
                                            <div class="input-group">
                                                <input type="text" id="HeaderID" class="form-control form-input-grey text-center font-weight-bold" ng-model="txtDiagHeaderID" required readonly />
                                                <div class="input-group-append">
                                                    <button type="button" class="btn btn-info" alt="default" data-toggle="modal" data-target="#DiagHead" href="javascript:void(0)" ng-click="GetHeadDiagAllData()"><i class="ti-search"></i></button>
                                                </div>
                                            </div>
                                        </div>
                                    </div>
                                    <div class="row p-t-20">
                                        <div class="col-sm-2">
                                            <label class="control-label text-dark" for="DiagnoseID">Diagnose ID:</label>
                                        </div>
                                        <div class="col-md-9">
                                            <div class="input-group">
                                                <input type="text" id="DiagnoseID" class="form-control form-input-grey text-center font-weight-bold" ng-model="txtDiagnoseID" required readonly />
                                                <div class="input-group-append">
                                                    <button type="button" class="btn btn-info" alt="default" data-toggle="modal" data-target="#DiagnoseDisease" href="javascript:void(0)" ng-click="GetDisAllData()"><i class="ti-search"></i></button>
                                                </div>
                                            </div>
                                        </div>
                                    </div>
                                    <div class="row p-t-20">
                                        <div class="col-sm-2">
                                            <label class="control-label text-dark" for="Description">Description:</label>
                                        </div>
                                        <div class="col-md-9">
                                            <div class="input-group">
                                                <input type="text" id="Description" class="form-control form-input-grey" ng-model="txtDescription" />
                                            </div>
                                        </div>
                                    </div>
                                    <div class="row p-t-20">
                                        <div class="col-sm-2">
                                            <label class="control-label text-dark" for="DiagnoseName">Diagnose Name:</label>
                                        </div>
                                        <div class="col-md-9">
                                            <div class="input-group">
                                                <input type="text" id="DiagnoseName" class="form-control form-input-grey" ng-model="txtDiagnoseName" required />
                                            </div>
                                        </div>
                                    </div>
                                    <div class="row p-t-20">
                                        <div class="col-sm-2">
                                            <label class="control-label text-dark" for="InActive2">InActive:</label>
                                        </div>
                                        <div class="col-md-9">
                                            <select id="Status2" ng-model="txtInActive2" class="btn btn-outline-secondary dropdown-toggle form-control form-input-grey">
                                                <option value="Active">Active</option>
                                                <option value="InActive">InActive</option>
                                            </select>
                                        </div>
                                    </div>
                                    <div class="row p-t-20">
                                        <div class="col-md-2"></div>
                                        <div class="col-md-3">
                                            <div class="form-group">
                                                <button ng-click="saveDiagdata()" ng-disabled="DiagnoseDisease.$invalid" class="btn btn-outline-success form-control"><i class="fa fa-check"></i>Save</button>
                                            </div>
                                        </div>
                                        <div class="col-md-3">
                                            <div class="form-group">
                                                <button ng-click="On_ClearDis()" class="btn btn-outline-warning form-control"><i class="icon-refresh"></i> Clear </button>
                                            </div>
                                        </div>
                                        <div class="col-md-3">
                                            <div class="form-group">
                                                <button ng-disabled="DiagnoseDisease.$invalid" ng-click="OnDeleteDis()" class="btn btn-outline-danger form-control"><i class="icon-trash"></i>Delete</button>
                                            </div>
                                        </div>
                                        <div class="col-md-3"></div>
                                    </div>
                                </form>
                            </div>
                        </div>
                            <div class="row">
                                <div class="col-lg-12">
                                    <div class="card">
                                        <div class="card-body">
                                            <form name="DiagnoseFrm">
                                                <div class="form-body form-group">
                                                    <div class="row p-t-20">
                                                        <div class="col-md-2"></div>
                                                        <div class="col-md-8">
                                                            <div id="accordian">
                                                                <div class="card m-b-0" ng-repeat="item in dtDiagnose2ViewGetAll">
                                                                    <a class="card-header text-decoration-none" id="heading11">
                                                                        <button class="btn btn-link" data-toggle="collapse" data-target="#G-{{item.HeaderID}}" ng-click="Get_DiagnoseViewdataByid(item.HeaderID)" aria-expanded="true" aria-controls="collapse1">
                                                                            <h5 class="m-b-0">{{$index + 1}}-{{item.HeaderName}}</h5>
                                                                        </button>
                                                                    </a>
                                                                    <div id="G-{{item.HeaderID}}" class="collapse show" aria-labelledby="heading11" data-parent="#accordian">
                                                                    </div>
                                                                </div>
                                                            </div>
                                                        </div>
                                                    </div>
                                                </div>
                                            </form>
                                        </div>
                                  </div>
                            </div>
                        </div>
                    </div>
                </div>
            </div>
        </div>
    </div>
    <!-- Row -->
</div>
@section scripts {

    <script>

        var RegistrationForm = angular.module("RegistrationForm", []);
        RegistrationForm.controller("Home", function ($scope, $http) {
            $scope.Notification = function (Type, Heading, Test) {
                //info, warning, success, error
                $.toast({
                    heading: Heading,
                    text: Test,
                    position: 'top-right',
                    loaderBg: '#ff6849',
                    icon: Type,
                    hideAfter: 3500,
                    stack: 6
                });
            };
     //=================Collapse Cards=========================
    //===============Display Diagnose 2 Related Data ==============
    $scope.GetDiagnose2AllData = function () {
        var displayReq = {
            method: "POST",
            url: "/DiagnoCategory/Get_DiagnoseViewdata",
            data: {}
        }
        $http(displayReq).then(function (Return) {
            $scope.dtDiagnose2ViewGetAll = angular.fromJson(Return.data);

        }, function myError(Return) {
            $scope.Notification('error', 'Error Code GEN0001', Return.data);
        });
    };
    //===============Display Diagnose 2 Related Data get by id==============
    $scope.Get_DiagnoseViewdataByid = function (id) {
        var displayReq = {
            method: "POST",
            url: "/DiagnoCategory/Get_DiagnoseViewdataByid",
            data: {
                HeaderID: id
            }
        }
        $http(displayReq).then(function (Return) {
            var dt = angular.fromJson(Return.data);
            var body = "";
            for (var i = 0; i < dt.length; i++) {
                body += "<div class=\"card-body\">";
                body += "<div class=\"box-title m-b-0 text-danger\">" + dt[i].DiseaseName + " </div>";
                body += "</div>";
            }
            document.getElementById("G-" + id).innerHTML = body;

        }, function myError(Return) {
            $scope.Notification('error', 'Error Code GEN0001', Return.data);
        });
    };
    //=================Collapse Cards End=====================
             //=============Get Header ID================
            $scope.GetHeadID = function () {
                var displayReq = {
                    method: "POST",
                    url: "/DiagnoCategory/Get_HeadID",
                    data: {
                    }
                }
                $http(displayReq).then(function (Return) {
                    var dt = angular.fromJson(Return.data);
                    if (dt.length > 0) {
                        $scope.txtHeaderID = dt[0].ID;
                    }
                    else {
                        $scope.txtHeaderID = '';
                    }

                }, function myError(Return) {
                    $scope.Notification('error', 'Error Code LC0001', Return.data);
                });
            };
            //=============Get Diagnose ID================
            $scope.GetDiagID = function () {
                var displayReq = {
                    method: "POST",
                    url: "/DiagnoCategory/Get_DiagID",
                    data: {
                    }
                }
                $http(displayReq).then(function (Return) {
                    var dt = angular.fromJson(Return.data);
                    if (dt.length > 0) {
                        $scope.txtDiagnoseID = dt[0].ID;
                    }
                    else {
                        $scope.txtDiagnoseID = '';
                    }

                }, function myError(Return) {
                    $scope.Notification('error', 'Error Code LC0001', Return.data);
                });
            };
             //=======Display Header All Data By lookup=================
            $scope.GetHeadAllData = function () {
                var displayReq = {
                    method: "POST",
                    url: "/DiagnoCategory/Get_Headdata",
                    data: {}
                }
                $http(displayReq).then(function (Return) {
                    $scope.On_ClearHead();
                    $scope.dtHeadGetAll = angular.fromJson(Return.data);
                }, function myError(Return) {
                    $scope.Notification('error', 'Error Code LC0001', Return.data);
                });
            };
            //=======Display Diagnose Header All Data By lookup=================
            $scope.GetHeadDiagAllData = function () {
                var displayReq = {
                    method: "POST",
                    url: "/DiagnoCategory/Get_Headdata",
                    data: {}
                }
                $http(displayReq).then(function (Return) {
                    $scope.dtHeadDiagGetAll = angular.fromJson(Return.data);
                }, function myError(Return) {
                    $scope.Notification('error', 'Error Code LC0001', Return.data);
                });
            };
            //=======Display Diagnose All Data By lookup=================
            $scope.GetDisAllData = function () {
                var displayReq = {
                    method: "POST",
                    url: "/DiagnoCategory/Get_Disdata",
                    data: {
                       ID:$scope.txtDiagHeaderID
                    }
                }
                $http(displayReq).then(function (Return) {
                    $scope.On_ClearDis();
                    $scope.dtDisaseGetAll = angular.fromJson(Return.data);
                }, function myError(Return) {
                    $scope.Notification('error', 'Error Code LC0001', Return.data);
                });
            };
            //===========Delete Header=============
            $scope.OnHeadDelete = function () {
                var displayReq = {
                    method: "POST",
                    url: "/DiagnoCategory/OnHeadDelete",
                    data: {
                        ID: $scope.txtHeaderID
                    }
                }
                $http(displayReq).then(function (Return) {
                    $scope.On_ClearDis();
                    $scope.txtDiagHeaderID = '';
                    $scope.On_ClearHead();
                    if (Return.data == "Delete") {
                        $scope.Notification('success', 'Operation Completed', 'Delete Header');
                    }
                    else {
                        $scope.Notification('error', 'Error Code CU1002', 'Please Contact to Admin');
                    }
                }, function myError(Return) {
                    $scope.On_Clear();
                    $scope.Notification('error', 'Error Code CU0002', 'Please Contact to Admin');
                });
            };
            //===========Delete Diagnose=============
            $scope.OnDeleteDis = function () {
                var displayReq = {
                    method: "POST",
                    url: "/DiagnoCategory/OnDisDelete",
                    data: {
                        ID: $scope.txtDiagnoseID
                    }
                }
                $http(displayReq).then(function (Return) {
                    $scope.On_ClearDis();
                    if (Return.data == "Delete") {
                        $scope.Notification('success', 'Operation Completed', 'Delete Diagnose');
                    }
                    else {
                        $scope.Notification('error', 'Error Code CU1002', 'Please Contact to Admin');
                    }
                }, function myError(Return) {
                    $scope.On_Clear();
                    $scope.Notification('error', 'Error Code CU0002', 'Please Contact to Admin');
                });
            };
            //============Header Data Get By ID============
            $scope.GetHeadDataByID = function (row) {
                var displayReq = {
                    method: "POST",
                    url: "/DiagnoCategory/Get_Headbyid",
                    data: {
                        ID: row
                    }
                }
                $http(displayReq).then(function (Return) {
                    var dt = angular.fromJson(Return.data);
                    if (dt.length > 0) {
                        $scope.txtHeaderID = dt[0].HeaderID;
                        $scope.txtHeaderName = dt[0].HeaderName;
                        //$scope.txtInActive = dt[0].InActive;
                        if (dt[0].InActive == false)
                        {
                            $scope.txtInActive = 'Active';
                        }
                        else
                        {
                            $scope.txtInActive = 'InActive';
                        }
                    }
                    else {
                        $scope.txtHeaderID = '';
                        $scope.txtHeaderName = '';
                        $scope.txtInActive = 'Active';
                    }

                }, function myError(Return) {
                    $scope.Notification('error', 'Error Code LC0001', Return.data);
                });
            };
            //============Diagnose Header Data Get By ID============
            $scope.GetHeadDiagDataByID = function (row) {
                var displayReq = {
                    method: "POST",
                    url: "/DiagnoCategory/Get_Headbyid",
                    data: {
                        ID: row
                    }
                }
                $http(displayReq).then(function (Return) {
                    var dt = angular.fromJson(Return.data);
                    if (dt.length > 0) {
                        $scope.txtDiagHeaderID = dt[0].HeaderID;
                    }
                    else {
                        $scope.txtDiagHeaderID = '';
                    }

                }, function myError(Return) {
                    $scope.Notification('error', 'Error Code LC0001', Return.data);
                });
            };
            //============Diagnose Data Get By ID============
            $scope.GetDisDataByID = function (row) {
                var displayReq = {
                    method: "POST",
                    url: "/DiagnoCategory/Get_Disdatabyid",
                    data: {
                        ID: row
                    }
                }
                $http(displayReq).then(function (Return) {
                    var dt = angular.fromJson(Return.data);
                    if (dt.length > 0) {
                        $scope.txtDiagnoseID = dt[0].DiseaseID;
                        $scope.txtDiagnoseName = dt[0].DiseaseName;
                        $scope.txtDescription = dt[0].Description;
                        //$scope.txtInActive2 = dt[0].InActive;
                        if (dt[0].InActive == false)
                        {
                            $scope.txtInActive2 = 'Active';
                        }
                        else
                        {
                            $scope.txtInActive2 = 'InActive';
                        }
                    }
                    else
                    {
                        $scope.txtDiagnoseID = '';
                        $scope.txtDescription = '';
                        $scope.txtDiagnoseName = '';
                        $scope.txtInActive2 = 'Active';
                    }

                }, function myError(Return) {
                    $scope.Notification('error', 'Error Code LC0001', Return.data);
                });
            };
            //=============Header Data Insert=================
            $scope.saveHeaddata = function () {
                var displayReq = {
                    method: 'POST',
                    url: '/DiagnoCategory/Add_HeadRecrd',
                    data: {
                        HeaderID:$scope.txtHeaderID,
                        HeaderName:$scope.txtHeaderName,
                        InActive:$scope.txtInActive
                    }
                }
                $http(displayReq).then(function (Return) {
                $scope.On_ClearHead();
              if (Return.data == "Inserted") {
              $scope.Notification('success', 'Operation Completed', 'Insert Header');
                       }
              else {
                $scope.Notification('error', 'Error Code CU1002', 'Please Contact to Admin');
                   }
                }, function myError(Return) {
                $scope.On_ClearHead();
             $scope.Notification('error', 'Error Code CU0002', 'Please Contact to Admin');
              });
            };
            //=============Diagnose Data Insert=================
            $scope.saveDiagdata = function () {
                var displayReq = {
                    method: 'POST',
                    url: '/DiagnoCategory/Add_DisRecrd',
                    data: {
                        HeaderID:$scope.txtDiagHeaderID,
                        DiseaseID:$scope.txtDiagnoseID,
                        DiseaseName: $scope.txtDiagnoseName,
                        Description:$scope.txtDescription,
                        InActive2:$scope.txtInActive2
                    }
                }
                $http(displayReq).then(function (Return) {
                    $scope.On_ClearDis();
              if (Return.data == "Inserted") {
              $scope.Notification('success', 'Operation Completed', 'Insert Diagnose');
                       }
              else {
                $scope.Notification('error', 'Error Code CU1002', 'Please Contact to Admin');
                   }
                }, function myError(Return) {
                $scope.On_ClearDis();
             $scope.Notification('error', 'Error Code CU0002', 'Please Contact to Admin');
              });
            };
            //===========Clear Function==============
            $scope.On_ClearHead = function () {
                $scope.GetHeadID();
                $scope.txtHeaderName = '';
                $scope.txtInActive = 'Active';
            };
            //===========Clear Function==============
