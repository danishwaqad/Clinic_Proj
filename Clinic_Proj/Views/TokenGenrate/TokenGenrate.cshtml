@{
    ViewBag.Title = "TokenGenrate";
}
<div ng-app="RegistrationForm" ng-controller="Home">
    <div class="row">
        <div class="col-lg-12">
            <div class="card">
                <div class="card-header bg-info">
                    <h4 class="m-b-0 text-white">Today Tokens Delete</h4>
                </div>
                <div id="Patient_TodayToken" class="modal fade" tabindex="-1" role="dialog" aria-labelledby="myLargeModalLabel" aria-hidden="true" style="display: none;">
                    <div class="modal-dialog modal-xl">
                        <div class="modal-content">
                            <div class="modal-header">
                                <h4 class="modal-title" id="myLargeModalLabel">Today Token Register</h4>
                                <button type="button" class="close" data-dismiss="modal" aria-hidden="true">×</button>
                            </div>
                            <div class="modal-body">
                                <div class="row">
                                    <div class="col-md-7">

                                    </div>
                                    <div class="col-md-1">
                                        <label for="CompSearch">Search:</label>
                                    </div>
                                    <div class="col-md-4 form-material">
                                        <input type="text" class="form-control" id="UserSearch" ng-model="TxtToday_TknSearch" />
                                    </div>
                                </div>
                                <div class="table-responsive-sm">
                                    <table class="table-sm table-hover table-bordered text-dark">
                                        <thead>
                                            <tr>
                                                <th>Sr#</th>
                                                <th>Token#</th>
                                                <th>CNIC</th>
                                                <th>Token Date</th>
                                                <th>FullName</th>
                                                <th>Gender</th>
                                                <th>Age</th>
                                                <th>SiteID</th>
                                            </tr>
                                        </thead>
                                        <tbody>
                                            <tr ng-repeat="item in dtToday_ToknGetAll | filter:TxtToday_TknSearch">
                                                <td>{{$index + 1}}</td>
                                                <td><button ng-click="Get_TodayTknByID(item.TokenNo)" class="btn btn-link" data-dismiss="modal">{{item.TokenNo}}</button></td>
                                                <td>{{item.CNIC}}</td>
                                                <td>{{item.TokenDate | date:dd-MMM-yyyy}}</td>
                                                <td>{{item.FullName}}</td>
                                                <td>{{item.Gender}}</td>
                                                <td>{{item.Age}}</td>
                                                <td>{{item.SiteID}}</td>
                                            </tr>
                                        </tbody>
                                    </table>
                                </div>
                            </div>
                            <div class="modal-footer">
                                <button type="button" class="btn btn-danger waves-effect text-left" data-dismiss="modal">Close</button>
                            </div>
                        </div>
                    </div>
                </div>
                        <form name="CreatePatient">
                            <div class="form-body form-group">
                                <div class="row p-t-20">
                                    <div class="col-md-1"></div>
                                    <div class="col-sm-1">
                                        <label class="control-label text-dark" for="CNIC">CNIC/Cell #:</label>
                                    </div>
                                    <div class="col-md-3">
                                        <div class="input-group">
                                            <input type="text" id="Cnic" class="form-control font-weight-bold" required ng-model="txtCNICNo" readonly />
                                            <div class="input-group-append">
                                                <button type="button" class="btn btn-info" alt="default" data-toggle="modal" data-target="#Patient_TodayToken" href="javascript:void(0)" ng-click="Get_ToknAllData()"><i class="ti-search"></i></button>
                                            </div>
                                        </div>
                                    </div>
                                    <div class="col-md-1"></div>
                                    <div class="col-sm-1">
                                        <label class="control-label text-dark" for="Relation">Relation:</label>
                                    </div>
                                    <div class="col-md-3">
                                        <div class="input-group">
                                            <input type="text" id="Relation" class="form-control form-input-grey" ng-model="txtRelation_Pat" readonly required />
                                        </div>
                                    </div>
                                </div>
                                <div class="row p-t-20">
                                    <div class="col-md-1"></div>
                                    <div class="col-sm-1">
                                        <label class="control-label text-dark" for="TokenNo">M.R #:</label>
                                    </div>
                                    <div class="col-md-3">
                                        <div class="input-group">
                                            <input type="text" id="TokenNo" class="form-control form-input-grey text-center font-weight-bold" ng-model="txtTokenNo_Pat" maxlength="100" required readonly />
                                        </div>
                                    </div>
                                    <div class="col-md-1"></div>
                                    <div class="col-sm-1">
                                        <label class="control-label text-dark" for="TknDate">Token Date:</label>
                                    </div>
                                    <div class="col-md-3">
                                        <div class="input-group">
                                            <input type="date" ng-model="txtTokenDate" class="form-control text-center font-weight-bold" readonly />
                                        </div>
                                    </div>
                                </div>
                                <h4 class="card-title"></h4>
                                <hr />
                                <div class="row p-t-20">
                                    <div class="col-md-1"></div>
                                    <div class="col-sm-1">
                                        <label class="control-label text-dark" for="PatientName">Patient Name:</label>
                                    </div>
                                    <div class="col-md-8">
                                        <div class="input-group">
                                            <select class="form-control form-input-grey col-md-2" ng-model="DDLTitle_Pat" readonly>
                                                <option value="Mr">Mr.</option>
                                                <option value="Master">Master.</option>
                                                <option value="Mrs">Mrs.</option>
                                                <option value="MS">Ms.</option>
                                                <option value="Baby">Baby.</option>
                                            </select>
                                            <input type="text" id="PatientFName" class="form-control form-input-grey" placeholder="First Name" ng-model="txtFPatientName_Pat" readonly required />
                                            <input type="text" id="PatientMName" class="form-control form-input-grey" placeholder="Middle Name" ng-model="txtMPatientName_Pat" readonly />
                                            <input type="text" id="PatientLName" class="form-control form-input-grey" placeholder="Last Name" ng-model="txtLPatientName_Pat" readonly />
                                        </div>
                                    </div>
                                </div>
                                <div class="row p-t-20">
                                    <div class="col-md-1"></div>
                                    <div class="col-sm-1">
                                        <label class="control-label text-dark" for="TknDelRemarks">Token Delete Remarks:</label>
                                    </div>
                                    <div class="col-md-8">
                                        <div class="input-group">
                                            <input type="text" id="TknDelRemarks" class="form-control form-input-grey" placeholder="Enter Must Token Delete Remarks" ng-model="txtDel_Remarks" />
                                        </div>
                                    </div>
                                </div>
                                <div class="row p-t-20">
                                    <div class="col-md-3"></div>
                                    <div class="col-md-2">
                                        <div class="form-group">
                                            <button ng-click="On_ClearTodayTkn()" class="btn btn-outline-warning form-control"><i class="icon-refresh"></i> Clear </button>
                                        </div>
                                    </div>
                                    <div class="col-md-2">
                                        <div class="form-group">
                                            <button ng-click="DeleteTodayTkn_Data()" ng-disabled="CreatePatient.$invalid" class="btn btn-outline-success form-control"><i class="fa fa-check"></i>Delete Token</button>
                                        </div>
                                    </div>
                                    <div class="col-md-2">
                                        <div class="form-group">
                                            <button ng-click="DeleteTodayTkn_GetBackData()" ng-disabled="CreatePatient.$invalid" class="btn btn-outline-success form-control"><i class="icon-reload"></i>Token Get Back</button>
                                        </div>
                                    </div>
                                    @*<div class="col-md-3"></div>*@
                                </div>
                            </div>
                        </form>
                        <hr />
            </div>
                        </div>
                    </div>
                </div>
@section scripts {
    <script src="~/Scripts/Custom/Update_Tokens.js"></script>
}




