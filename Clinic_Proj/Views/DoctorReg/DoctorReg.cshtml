                $http(displayReq).then(function (Return) {

                    $scope.dtGetShareAll = angular.fromJson(Return.data);

                }, function myError(Return) {
                    $scope.Notification('error', 'Error Code LC0001', Return.data);
                });
            };
            //======================Get Dr Type By ID=========================
            $scope.GetdrByType = function (DoctorType) {
                //if (DoctorType == 'Sharing - Monthly' || DoctorType == 'Sharing - Daily' || DoctorType == 'Salary + Sharing' || DoctorType == 'Visiting') {
                //    $scope.ShareTxt = false;
                    $scope.txtDoctorType = DoctorType;
                //}
                //else {
                //    $scope.ShareTxt = true;
                //    $scope.txtDoctorType = DoctorType;
                //}
            };
            //==================Display Dr Type Data By lookup============================
            $scope.GetDrType = function () {
                var displayReq = {
                    method: "POST",
                    url: "/DoctorReg/Get_DrdataType",
                    data: {
                    }
                }
                $http(displayReq).then(function (Return) {
                    $scope.dtGetTypeAll = angular.fromJson(Return.data);
                }, function myError(Return) {
                    $scope.Notification('error', 'Error Code LC0001', Return.data);
