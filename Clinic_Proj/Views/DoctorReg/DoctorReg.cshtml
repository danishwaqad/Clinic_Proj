            $scope.GetDataByID = function (Doctor,Site) {
                var displayReq = {
                    method: "POST",
                    url: "/DoctorReg/Get_databyid",
                    data: {
                        DoctorID: Doctor,
                        SiteID: Site
                    }
                }
                $http(displayReq).then(function (Return) {

                    var dt = angular.fromJson(Return.data);

                    if (dt.length > 0) {
                        $scope.txtDoctorID = dt[0].DoctorID;
                        $scope.GetViewAllShareData(dt[0].DoctorID,dt[0].SiteID);
                        $scope.txtDoctorType = dt[0].DoctorType;
                        $scope.txtDrName = dt[0].DoctorName;
                        $scope.txtDrFName = dt[0].DrFName;
                        $scope.txtCNIC = dt[0].CNIC;
                        $scope.txtTel1 = dt[0].Tel1;
                        $scope.txtTel2 = dt[0].Tel2;
                        $scope.txtEmailID = dt[0].EmailID;
                        $scope.txtAddress = dt[0].Address;
                        $scope.txtEducation = dt[0].DoctorEdu;
                        $scope.txtCity = dt[0].City;
                        $scope.txtCountry = dt[0].Country;
                        $scope.txtGender = dt[0].Gender;
                        $scope.txtDOB = new Date(dt[0].DOB);
                        $scope.txtDescription = dt[0].Description;
                        $scope.txtRemarks = dt[0].Remarks;
                        $scope.txtReferBy = dt[0].Designation;
                        $scope.txtDivisionCode = dt[0].DivisionID;
                        $scope.txtSiteCode = dt[0].SiteID;
                        $scope.txtLoginID = dt[0].LoginID;
                        $scope.txtStatus = dt[0].Status;
                        if (dt[0].HomeStatus == 1) {
                            $scope.txtHomeStatus = 'Yes';
                        }
                        else {
                            $scope.txtHomeStatus = 'No';
                        }
                        $scope.txtDrShift = dt[0].DrShift;
                        $scope.txtCharges = dt[0].Charges;
                        $scope.TxtUsername = dt[0].UserName;
                        $scope.TxtPassword = dt[0].Pass;
                        $scope.CUSaveBtn = true;
                        $scope.CUUpdateBtn = false;
                        $scope.Delbtn = false;
                    }
                    else {
                        $scope.CUSaveBtn = false;
                        $scope.CUUpdateBtn = true;
                        $scope.Delbtn = true;

                        $scope.txtDoctorType = '';
                        $scope.txtDrName = '';
                        $scope.ShareTxt = true;
                        $scope.txtSharePercnt = 0;
                        $scope.txtDrFName = '';
                        $scope.txtCNIC = '';
                        $scope.txtTel1 = '';
                        $scope.txtTel2 = '';
                        $scope.txtEmailID = '';
                        $scope.txtAddress = '';
                        $scope.txtEducation = '';
                        $scope.txtCity = '';
                        $scope.txtCountry = '';
                        $scope.txtGender = '1';
                        $scope.txtDOB = '';
                        $scope.txtDescription = '';
                        $scope.txtRemarks = '';
                        $scope.txtReferBy = '';
                        $scope.txtDivisionCode = '';
                        $scope.txtSiteCode = '';
                        $scope.txtLoginID = '';
                        $scope.txtStatus = 'Active';
                        $scope.txtHomeStatus = 'No'

                        $scope.txtDrShift = '';
                        $scope.txtCharges = '';
                        $scope.TxtUsername = '';
                        $scope.TxtPassword = '';
                    }

                }, function myError(Return) {
                    $scope.Notification('error', 'Error Code LC0001', Return.data);
                });
            };
            //================Delete Doctor=============================
            $scope.OnDelete = function () {
                var displayReq = {
                    method: "POST",
                    url: "/DoctorReg/OnDelete",
                    data: {
                        DoctorID: $scope.txtDoctorID,
                        SiteID: $scope.txtSiteCode
                    }
                }
                $http(displayReq).then(function (Return) {
                    $scope.On_Clear();
                    if (Return.data == "Delete") {
                        $scope.Notification('success', 'Operation Completed', 'Delete Doctor');
                    }
                    else {
                        $scope.Notification('error', 'Error Code CU1002', 'Please Contact to Admin');
                    }
                }, function myError(Return) {
                    $scope.On_Clear();
                    $scope.Notification('error', 'Error Code CU0002', 'Please Contact to Admin');
                });

            };
            //===============Dr Shift Select By ID===================
            $scope.GetdrByShift = function (ShiftID) {
                $scope.txtDrShift = ShiftID;
            };
            //===============Dr Share Select By ID===================
            $scope.GetdrByShare = function (ShareID) {
                $scope.txtShareType = ShareID;
                $scope.GetViewShareDataByID($scope.txtDoctorID,ShareID,$scope.txtSiteCode);
            };
            //===============Get Dr shift LookUp Data===================
            $scope.GetDrShift = function () {
                var displayReq = {
                    method: "POST",
                    url: "/DoctorReg/Get_DrdataShift",
                    data: {
                    }
