                    data: {
                        DoctorID: DocID,
                        SiteID: SiteID,
                        ShareType:ShareID
                    }
                }
                $http(displayReq).then(function (Return) {

                    var dt = angular.fromJson(Return.data);

                    if (dt.length > 0) {
                        $scope.txtShareType = dt[0].ShareType;
                        $scope.txtSharePercnt = dt[0].SharePercent;
                    }
                    else {
                        //$scope.txtShareType = '';
                        $scope.txtSharePercnt = 0;
                    }

                }, function myError(Return) {
                    $scope.Notification('error', 'Error Code LC0001', Return.data);
                });
            };
            //============Delete Doctor Share===============
            $scope.OnDocShareDelete = function (DocID,ShareID,SiteID) {
                var displayReq = {
                    method: "POST",
                    url: "/DoctorReg/OnShare_Delete",
                    data: {
                        DoctorID: DocID,
                        SiteID: SiteID,
                        ShareType:ShareID
                    }
                }
                $http(displayReq).then(function (Return) {
                    if (Return.data == "Delete") {
                        $scope.GetViewAllShareData($scope.txtDoctorID, $scope.txtSiteCode);
                        $scope.txtShareType = '';
                        $scope.txtSharePercnt = 0;
                        $scope.Notification('success', 'Operation Completed', 'Delete Doctor Share');
                    }
                    else {
                        $scope.Notification('error', 'Error Code CU1002', 'Please Contact to Admin');
                    }
                }, function myError(Return) {
                    $scope.On_Clear();
                    $scope.Notification('error', 'Error Code CU0002', 'Please Contact to Admin');
                });
            };
            //==================Site Data Get By ID=======================
            $scope.GetSiteDataByID = function (ID) {
                var displayReq = {
                    method: "POST",
                    url: "/Site/Get_databyid",
                    data: {
                        SiteCode: ID
                    }
                }
                $http(displayReq).then(function (Return) {
                    var dt = angular.fromJson(Return.data);
                    if (dt.length > 0) {
                        $scope.txtSiteCode = dt[0].SiteCode;
                    }
                    else {
                        $scope.txtSiteCode = '';
                    }
                }, function myError(Return) {
                    $scope.Notification('error', 'Error Code LC0001', Return.data);
                });
            };
            //=============Display Doctor Data By LookUp=================
