                        DrFName: $scope.txtDrFName,
                        CNIC: $scope.txtCNIC,
                        Tel1: $scope.txtTel1,
                        Tel2: $scope.txtTel2,
                        EmailID: $scope.txtEmailID,
                        Address: $scope.txtAddress,
                        Education:$scope.txtEducation,
                        City: $scope.txtCity,
                        Country: $scope.txtCountry,
                        Gender: $scope.txtGender,
                        DOB: $scope.txtDOB,
                        Description: $scope.txtDescription,
                        Remarks: $scope.txtRemarks,
                        ReferBy: $scope.txtReferBy,
                        DivisionID: $scope.txtDivisionCode,
                        SiteID: $scope.txtSiteCode,
                        LoginID: $scope.txtLoginID,
                        Status: $scope.txtStatus,
                        HomeStatus: $scope.txtHomeStatus,
                        DrShift: $scope.txtDrShift,
                        Charges: $scope.txtCharges,
                        UserName: $scope.TxtUsername,
                        Pass: $scope.TxtPassword,
                        SharePercent: $scope.txtSharePercnt
                    }
                }
                $http(displayReq).then(function (Return) {
                    if (Return.data == "Updated") {
                        $scope.Notification('success', 'Operation Completed', 'Update Doctor');
                        $scope.On_Clear();
                    }
                    else {
                        $scope.Notification('error', 'Error Code CU1002', 'Please Contact to Admin');
                    }
                }, function myError(Return) {
                    $scope.On_Clear();
                    $scope.Notification('error', 'Error Code CU0002', 'Please Contact to Admin');
                });
            };
            //===================Auto Get Dr ID===============================
            $scope.GetDrID = function () {
                var displayReq = {
                    method: "POST",
                    url: "/DoctorReg/Get_DrID",
                    data: {
                    }
                }
                $http(displayReq).then(function (Return) {
                    var dt = angular.fromJson(Return.data);
                    if (dt.length > 0) {
                        $scope.txtDoctorID = dt[0].ID;
                    }
                }, function myError(Return) {
                    $scope.Notification('error', 'Error Code LC0001', Return.data);
                });
            };
            //=============================Clear Function==============================
            $scope.On_Clear = function () {
                $scope.txtDoctorType = '';
                $scope.txtDrName = '';
                $scope.txtDrFName = '';
                $scope.txtCNIC = '';
                $scope.txtShareType = '';
                $scope.txtTel1 = '';
                $scope.txtTel2 = '';
                $scope.txtEmailID = '';
                $scope.txtAddress = '';
                $scope.txtEducation = '';
                $scope.txtCity = '';
                $scope.txtCountry = '';
                $scope.txtDescription = '';
                $scope.txtRemarks = '';
                $scope.txtReferBy = '';
                $scope.txtDivisionCode = '';
                $scope.txtSiteCode = '';
                $scope.txtLoginID = '';
                $scope.txtDrShift = '';
                $scope.TxtUsername = '';
