@section scripts {
    <script type="text/javascript">
        //CNIC Pattern
        function addHyphen(elem) {
            if (elem.value.length <= 14) {
                if (elem.value.length === 5) {
                    elem.value = `${elem.value}-`;
                }
                if (elem.value.length === 13) {
                    elem.value = `${elem.value}-`;
                }
                if (elem.value.length === 15) {
                    elem.value = `${elem.value}`;
                }
            } else {
                elem.value = elem.value.slice(0, -1)
            }
        };
    </script>
    <script>

        var RegistrationForm = angular.module("RegistrationForm", []);
        RegistrationForm.controller("Home", function ($scope, $http) {
            $scope.Notification = function (Type, Heading, Test) {
                //info, warning, success, error
                $.toast({
                    heading: Heading,
                    text: Test,
                    position: 'top-right',
                    loaderBg: '#ff6849',
                    icon: Type,
                    hideAfter: 3500,
                    stack: 6
                });
            };
            //================Display DTall site data=====================
            $scope.GetSiteAllData = function () {
                var displayReq = {
                    method: "POST",
                    url: "/Site/Get_data",
                    data: {}
                }
                $http(displayReq).then(function (Return) {

                    $scope.dtGetSiteAll = angular.fromJson(Return.data);

                }, function myError(Return) {
                    $scope.Notification('error', 'Error Code LC0001', Return.data);
                });
            };
            //=================Display View All Doctor Share Data===================
            $scope.GetViewAllShareData = function (DrID,SiteID) {
                var displayReq = {
                    method: "POST",
                    url: "/DoctorReg/Get_ShareViewdata",
                    data: {
                        DoctorID: DrID,
                        SiteID:SiteID
                    }
                }
                $http(displayReq).then(function (Return) {

                    $scope.dtShareViewGetAll = angular.fromJson(Return.data);

                }, function myError(Return) {
                    $scope.Notification('error', 'Error Code LC0001', Return.data);
                });
            };
