                $scope.txtGender = 'Male';
                $scope.txtStatus = 'Active';
                $scope.txtHomeStatus = 'No'
                $scope.txtDOB = new Date('01/01/1900');
                $scope.txtCharges = 0;
                $scope.ShareTxt = true;
                $scope.txtSharePercnt = 0;
            };
            $scope.OnLoad();
            $scope.GetDrID();
        });


    </script>

}
